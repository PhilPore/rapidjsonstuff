{
    "$schema" : "http://json-schema.org/draft-07/schema",
    "$id" : "Tobeannounced",
    "title" : "Schemes",
    "Description" : "All Schemes within PALISADE (please ignore content in id for now)",
    "type" : "object",
    "properties" : {
        "Scheme Name" : {
            "description" : "Name of function",
            "type" : "string"
        },
        "Return Type" : {
            "description" : "Data type that the function returns",
            "type" : "string"
        },
        "Parameters" : {
            "description" : "The variables that the function requires",
            "type" : "array"
        },
        "Inheritance" : {
            "description" : "The file that the function came from",
            "type" : "string"
        },
        "Other Schemes" : {
            "description" : "Files that calls the function",
            "type" : "array"
        }
    },
    "required" : ["Return Type","Function Name","Inheritance"]
}